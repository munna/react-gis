{
  "name": "react-gis",
  "version": "1.0.0",
  "description": "A starter project for GIS Application in react with openLayer",
  "main": "src/index.js",
  "scripts": {
    "start": "node index.js",
    "pack": "webpack --config webpack/webpack.dev.config.js",
    "babel": "babel src -d views",
    "build": "npm run pack && npm run babel"
  },
  "babel": {
    "presets": [
      "es2015",
      "react",
      "stage-0"
    ],
    "plugins": [
      "transform-object-rest-spread"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/munna/react-gis.git"
  },
  "keywords": [
    "React",
    "GIS"
  ],
  "author": "Munna Bhakta",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/munna/react-gis/issues"
  },
  "homepage": "https://github.com/munna/react-gis#readme",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.5",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-register": "^6.26.0",
    "css-loader": "^1.0.0",
    "less": "^3.8.0",
    "less-loader": "^4.1.0",
    "react-openlayers": "^0.2.1",
    "style-loader": "^0.21.0",
    "webpack-cli": "^3.1.0"
  },
  "dependencies": {
    "express": "^4.16.3",
    "express-fileupload": "^0.4.0",
    "interactjs": "^1.3.4",
    "ol": "^5.0.0",
    "prop-types": "^15.6.2",
    "react": "^16.4.1",
    "react-dom": "^16.4.1",
    "react-redux": "^5.0.7",
    "redux": "^4.0.0",
    "redux-thunk": "^2.3.0",
    "webpack": "^4.16.3",
    "webpack-dev-server": "^3.1.5"
  }
}
